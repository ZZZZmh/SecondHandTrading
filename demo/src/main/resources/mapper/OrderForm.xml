<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zmh.secondHandTrading.mapper.OrderFormMapper">
    <insert id="addOrderForm" parameterType="map">
        insert into order_form(
        order_id,commodity_id,buyer,purchaseQuantity,address,status,create_time)
        values(#{orderId},#{commodityId},#{buyer},#{purchaseQuantity},#{address},#{status},#{createTime})
	</insert>
</mapper>